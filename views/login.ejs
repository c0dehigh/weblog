<form class="form-signin">
  <h1 class="h3 mb-3 font-weight-normal">ورد به بخش مدیریت</h1>
  <label for="inputEmail" class="sr-only">آدرس ایمیل</label>
  <input
    type="email"
    id="inputEmail"
    class="form-control"
    placeholder="ایمیل"
    required
    autofocus
  />
  <label for="inputPassword" class="sr-only">کلمه عبور</label>
  <input
    type="password"
    id="inputPassword"
    class="form-control"
    placeholder="کلمه عبور"
    required
  />
  <div class="checkbox mb-3">
    <label>
      <input type="checkbox" value="remember-me" /> من رو به خاطر بسپار
    </label>
  </div>
  <button class="btn btn-lg btn-primary btn-block" type="submit">
    ورود
  </button>
  
</form>
